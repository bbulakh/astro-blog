import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import TrendingSlider from "../components/TrendingSlider.astro";
import { SITE_TITLE, SITE_DESCRIPTION } from "../consts";
---

<!doctype html>
<html class="bg-white dark:bg-[#003049]" lang="en">
  <head>
    <BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
  </head>
  <body>
    <Header title={SITE_TITLE} />

    <TrendingSlider title="Unlocking the Potential of Blockchain Technology in Transforming Supply Chain Management"/>

    <main class="max-w-[1200px] mx-auto">
      
    </main>
    <Footer />
  </body>
</html>
